{
  "name": "vite-react-ts-template",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "dev": "vite",
    "build": "tsc && yarn build:formatjs && yarn build:client && yarn build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:formatjs": "yarn formatjs:extract && yarn formatjs:compile && yarn lint --fix",
    "generate": "vite build --outDir dist/static && yarn build:server && node prerender",
    "serve": "node server",
    "formatjs:extract": "formatjs extract 'src/**/*.ts*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "formatjs:compile": "formatjs compile lang/en.json --ast --out-file lang/compiled/en.json"
  },
  "dependencies": {
    "@headlessui/react": "^1.0.0",
    "@heroicons/react": "^1.0.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.0.9",
    "react-intl": "^5.15.8",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.2.7",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-formatjs": "^2.14.6",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.2.4",
    "vite": "^2.1.5",
    "vite-plugin-fonts": "^0.2.2"
  },
  "prettier": {
    "singleQuote": true
  }
}
